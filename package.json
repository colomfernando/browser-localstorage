{
	"name": "browser-localstorage-expire",
	"version": "1.6.0",
	"description": "package for manage localstorage browser",
	"main": "dist/index.js",
	"module": "dist/index.esm.js",
	"types": "dist/index.d.ts",
	"files": [
		"dist/**"
	],
	"scripts": {
		"test": "jest",
		"docs": "jsdoc2md --template template-readme.hbs --configure ./jsdoc2md.json --files src/index.ts > README.md",
		"build": "rollup -c",
		"lint": "lint-staged"
	},
	"husky": {
		"hooks": {
			"pre-commit": "npm run lint"
		}
	},
	"lint-staged": {
		"*{js, json}": [
			"prettier --write",
			"eslint --fix"
		],
		"*.js": [
			"jest --bail --findRelatedTests"
		]
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/colomfernando/browser-localstorage-expire.git"
	},
	"keywords": [
		"browser",
		"localstorage",
		"cache"
	],
	"author": "Fernando Colom",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/colomfernando/browser-localstorage-expire/issues"
	},
	"homepage": "https://github.com/colomfernando/browser-localstorage-expire#readme",
	"devDependencies": {
		"@babel/cli": "^7.8.4",
		"@babel/core": "^7.9.6",
		"@babel/plugin-proposal-object-rest-spread": "^7.9.6",
		"@babel/plugin-transform-runtime": "^7.9.6",
		"@babel/preset-env": "^7.9.6",
		"@babel/preset-typescript": "^7.16.7",
		"@babel/register": "^7.9.0",
		"@rollup/plugin-node-resolve": "^7.1.3",
		"@types/jest": "^27.4.1",
		"@typescript-eslint/eslint-plugin": "^5.14.0",
		"@typescript-eslint/parser": "^5.14.0",
		"babel-core": "^6.26.3",
		"babel-eslint": "^10.1.0",
		"babel-jest": "^25.5.1",
		"eslint": "^8.11.0",
		"eslint-config-airbnb-base": "^14.1.0",
		"eslint-config-prettier": "^6.11.0",
		"eslint-plugin-babel": "^5.3.0",
		"eslint-plugin-import": "^2.20.2",
		"eslint-plugin-jest": "^23.8.2",
		"eslint-plugin-jsx-a11y": "^6.2.3",
		"eslint-plugin-prettier": "^3.1.3",
		"husky": "^4.2.5",
		"jest": "^27.5.1",
		"jest-localstorage-mock": "^2.4.2",
		"jsdoc": "^3.6.4",
		"jsdoc-babel": "^0.5.0",
		"jsdoc-to-markdown": "^5.0.3",
		"lint-staged": "^10.2.2",
		"prettier": "^2.0.5",
		"rollup": "^2.7.6",
		"rollup-babel": "^0.6.3",
		"rollup-plugin-babel": "^4.3.2",
		"rollup-plugin-commonjs": "^10.1.0",
		"rollup-plugin-node-resolve": "^5.2.0",
		"rollup-plugin-peer-deps-external": "^2.2.2",
		"ts-jest": "^27.1.3",
		"typescript": "^4.6.2"
	}
}
